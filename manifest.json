{
  "manifest_version": 3,
  "name": "Deep Code Extension",
  "version": "1.0",
  "description": "Chrome extension for code suggestions in telescope and tessalator",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://vrdn-ai.89determined.workers.dev/",
    "*://kmeconline.com/*",
    "*://*.teleuniv.in/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Deep Code Extension",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    
  }
}